@ARTICLE{Abramovitz1997,
  author = {Dana L. Abramovitz and Anna Marie Pyle},
  title = {Remarkable morphological variability of a common {RNA} folding motif:
	the {GNRAT}etraloop-receptor interaction},
  journal = {Journal of Molecular Biology},
  year = {1997},
  volume = {266},
  pages = {493-506},
  number = {3},
  abstract = {One of the most common {RNA} tertiary interactions involves the docking
	of {GNRA} hairpin loops into stem-loop structures on other regions
	of {RNA}. {D}omain 5 of the group {II} intron interacts with {D}omain
	1 through such an interaction, which has been characterized thermodynamically
	and kinetically for the ai5g intron. {U}sing this system, it was
	possible to test the morphological tolerances of the {GNRA} tetraloop
	involved in tertiary interactions. {T}he data presented herein show
	that a {GNRA} tetraloop can still participate in tertiary interaction
	after being physically cut at any phosphodiester linkage within the
	loop. {T}he "nicked tetraloop" can be expanded by many nucleotides
	in either direction and the covalently continuous loop can also be
	expanded without loss of interaction energy. {I}n the context of
	the nicked tetraloop, the second nucleotide of the tetraloop sequence
	can be completely deleted without loss of function. {B}y examining
	radical alterations in tetraloop structure, this study helps define
	the minimal sequence and structural requirements of a {GNRA} motif
	involved in long-range tertiary interaction. {I}t shows that "tetraloop"-like
	structures capable of forming tertiary interactions can be imbedded
	in unexpected contexts, such as internal loops and apparently open
	structure within {RNA}. {I}t demonstrates that pentaloops and hexaloops
	can form the same type of interaction, with almost equal affinity,
	as a tetraloop. {T}aken together, these data suggest a more generic
	term for the {GNRA} tetraloop-receptor interaction: {I}t is proposed
	herein that the term "{GNRA} tetraloop" be replaced by "{GN}n/{RA}",
	where n represents a variable number of nucleotides and / indicates
	that the loop can be divided and interrupted by other sequences.},
  doi = {10.1006/jmbi.1996.0810},
  file = {D\:\\Articles\\abramovitz1997.pdf:D\:\\Articles\\abramovitz1997.pdf:PDF},
  keywords = {GNRA tetraloop; RNA folding; RNA tertiary interaction; group II intron;
	ribozyme},
  owner = {esguerra}
}

